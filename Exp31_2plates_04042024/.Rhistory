"0uM" = "0 uM TPEN",
"18uM" = "18 uM TPEN",
"24uM" = "24 uM TPEN",
"30uM" = "30 uM TPEN",
"36uM" = "36 uM TPEN",
"42uM" = "42 uM TPEN",
"48uM" = "48 uM TPEN",
"54uM" = "54 uM TPEN",
"60uM" = "60 uM TPEN",
"70uM" = "70 uM TPEN"))
Exp30_v5  %>% ggplot(aes(x = Time_h,
y = ODmean,
color = TPEN_or_ZnCl2_conc,
fill = TPEN_or_ZnCl2_conc
)) +
geom_point() +
geom_line() +
geom_ribbon(aes(ymin=ODmean-sdOD, ymax=ODmean+sdOD),
alpha = 0.3, colour = NA) +
facet_wrap (Community ~ Zn_condition) +
theme_bw()
